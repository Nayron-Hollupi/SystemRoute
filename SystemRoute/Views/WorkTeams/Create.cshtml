@model Route.Domain.Model.WorkTeam

@{
    ViewData["Title"] = "Cadastrar Equipe";
}

<div class="row login">
    <h1>Cadastrar Equipe</h1>
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <label asp-for="Person" class="control-label"></label>
                <input asp-for="Person" class="form-control" />
                <span asp-validation-for="Person" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group d-grid gap-2">
    
                <input type="submit" value="Enviar" class="btn btn-primary" />
            </div>
        </form>
    </div>
    <div>
        <br />
    <a asp-action="Index">Visualizar Cidades cadastradas</a>
</div>
</div>



